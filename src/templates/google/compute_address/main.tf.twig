resource "google_compute_address" "{{ name }}" {
  name         = "${var.google_compute_address_name}"
  subnetwork   = "${var.google_subnetwork}"
  address_type = "${var.google_compute_address_type}"
  address      = "${var.google_compute_address_ip}"
  region       = "${var.google_region}"
  description  = "${var.google_compute_address_description}"
  network_tier = "${var.google_compute_address_network_tier}"
  project      = "${var.google_project_id}"

  labels = "${merge(var.default_labels, var.custom_labels)}"
}