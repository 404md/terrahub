  template:
    # terraform:
    #   backend:
    #     bucket: 'tf-state-prod'
    #     prefix: 'terraform/state'
    resource:
      google_compute_target_tcp_proxy:
        {{ name }}:
          backend_service: '${var.{{ name }}_backend_service}'
          name: '${var.{{ name }}_name}'
          # project: ''
          # description: ''
          # proxy_header: ''
    variable:
      {{ name }}_backend_service:
        type: 'string'
      {{ name }}_name:
        type: 'string'
    output:
      id:
        value: '${google_compute_target_tcp_proxy.{{ name }}.id}'
      thub_id:
        value: '${google_compute_target_tcp_proxy.{{ name }}.id}'
      proxy_id:
        value: '${google_compute_target_tcp_proxy.{{ name }}.proxy_id}'
      project:
        value: '${google_compute_target_tcp_proxy.{{ name }}.project}'
      self_link:
        value: '${google_compute_target_tcp_proxy.{{ name }}.self_link}'
      creation_timestamp:
        value: '${google_compute_target_tcp_proxy.{{ name }}.creation_timestamp}'
