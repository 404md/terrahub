# Define list of variables to be output

output "name" {
  value = "${google_service_account_key.{{ name }}.name}"
}

output "public_key" {
  value = "${google_service_account_key.{{ name }}.public_key}"
}

output "private_key" {
  value = "${google_service_account_key.{{ name }}.private_key}"
}

output "valid_after" {
  value = "${google_service_account_key.{{ name }}.valid_after}"
}

output "valid_before" {
  value = "${google_service_account_key.{{ name }}.valid_before}"
}
