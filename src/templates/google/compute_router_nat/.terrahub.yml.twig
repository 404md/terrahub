    resource:
      google_compute_router_nat:
        {{ name }}:
          nat_ip_allocate_option: '${var.{{ name }}_nat_ip_allocate_option}'
          name: '${var.{{ name }}_name}'
          router: '${var.{{ name }}_router}'
          # source_subnetwork_ip_ranges_to_nat: ''
          # tcp_established_idle_timeout_sec: 0
          # region: ''
          # nat_ips: []
          # subnetwork:
          # min_ports_per_vm: 0
          # udp_idle_timeout_sec: 0
          # icmp_idle_timeout_sec: 0
          # tcp_transitory_idle_timeout_sec: 0
          # project: ''
    variable:
      {{ name }}_nat_ip_allocate_option:
        type: 'string'
      {{ name }}_name:
        type: 'string'
      {{ name }}_router:
        type: 'string'
    output:
      id:
        value: '${google_compute_router_nat.{{ name }}.id}'
      thub_id:
        value: '${google_compute_router_nat.{{ name }}.id}'
      region:
        value: '${google_compute_router_nat.{{ name }}.region}'
      project:
        value: '${google_compute_router_nat.{{ name }}.project}'
