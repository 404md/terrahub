resource "google_compute_backend_service" "{{ name }}" {
  name        = "${var.google_compute_backend_service_name}"
  description = "${var.google_compute_backend_service_description}"
  port_name   = "${var.google_compute_backend_service_port_name}"
  protocol    = "${var.google_compute_backend_service_protocol}"
  timeout_sec = "${var.google_compute_backend_service_timeout_sec}"
  enable_cdn  = "${var.google_compute_backend_service_enable_cdn}"
  project     = "${var.google_project_id}"

  backend {
    group = "${var.google_compute_backend_service_backend_group}"
  }

  health_checks = ["${var.google_compute_backend_service_health_checks}"]
}