# top level #
#############
google_compute_forwarding_rule_name         = "{{ name }}"
google_compute_forwarding_rule_description  = "Managed by TerraHub"
google_compute_forwarding_rule_ip_address   = "10.0.0.1"
google_compute_forwarding_rule_ip_protocol  = "TCP"
google_compute_forwarding_rule_ip_version   = "IPV4"
