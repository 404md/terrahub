# Define list of variables to be output

output "id" {
  value = "${aws_nat_gateway.{{ name }}.id}"
}

output "thub_id" {
  value = "${aws_nat_gateway.{{ name }}.thub_id}"
}

output "allocation_id" {
  value = "${aws_nat_gateway.{{ name }}.allocation_id}"
}

output "subnet_id" {
  value = "${aws_nat_gateway.{{ name }}.subnet_id}"
}

output "network_interface_id" {
  value = "${aws_nat_gateway.{{ name }}.network_interface_id}"
}

output "private_ip" {
  value = "${aws_nat_gateway.{{ name }}.private_ip}"
}

output "public_ip" {
  value = "${aws_nat_gateway.{{ name }}.public_ip}"
}