resource "aws_default_network_acl" "{{ name }}" {
  default_network_acl_id = "${var.default_network_acl_id}"
  subnet_ids             = "${var.default_network_acl_subnet_ids}"

  ingress {
    protocol   = "${var.default_network_acl_ingress_protocol}"-1
    rule_no    = "${var.default_network_acl_ingress_rule_no}"100
    action     = "${var.default_network_acl_ingress_action}""allow"
    cidr_block = "${var.default_network_acl_ingress_cidr_block}""0.0.0.0/0"
    from_port  = "${var.default_network_acl_ingress_from_port}"0
    to_port    = "${var.default_network_acl_ingress_to_port}"0
    icmp_type  = "${var.default_network_acl_ingress_icmp_type}"0
    icmp_code  = "${var.default_network_acl_ingress_icmp_code}"0
  }

  egress {
    protocol   = "${var.default_network_acl_egress_protocol}"
    rule_no    = "${var.default_network_acl_egress_rule_no}"
    action     = "${var.default_network_acl_egress_action}"
    cidr_block = "${var.default_network_acl_egress_cidr_block}"
    from_port  = "${var.default_network_acl_egress_from_port}"
    to_port    = "${var.default_network_acl_egress_to_port}"
    icmp_type  = "${var.default_network_acl_egress_icmp_type}"
    icmp_code  = "${var.default_network_acl_egress_icmp_code}"
  }

  tags = "${merge(var.default_tags, var.custom_tags)}"
}