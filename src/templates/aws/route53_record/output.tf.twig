# Define list of variables to be output

output "fqdn" {
  value = "${aws_route53_record.{{ name }}.fqdn}"
}

output "name" {
  value = "${aws_route53_record.{{ name }}.name}"
}

output "type" {
  value = "${aws_route53_record.{{ name }}.type}"
}

output "records" {
  value = "${aws_route53_record.{{ name }}.records}"
}
