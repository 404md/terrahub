# Define list of variables to be output

output "arn" {
  value = "${aws_elasticsearch_domain.{{ name }}.arn}"
}

output "domain_id" {
  value = "${aws_elasticsearch_domain.{{ name }}.domain_id}"
}

output "domain_name" {
  value = "${aws_elasticsearch_domain.{{ name }}.domain_name}"
}

output "endpoint" {
  value = "${aws_elasticsearch_domain.{{ name }}.endpoint}"
}

output "kibana_endpoint" {
  value = "${aws_elasticsearch_domain.{{ name }}.kibana_endpoint}"
}
