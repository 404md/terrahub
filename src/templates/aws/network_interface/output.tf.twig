# Define list of variables to be output

output "eni_id" {
  value = "${aws_network_interface.{{ name }}.id}"
}

output "subnet_id" {
  value = "${aws_network_interface.{{ name }}.subnet_id}"
}

output "private_ips" {
  value = "${aws_network_interface.{{ name }}.private_ips}"
}

output "security_groups" {
  value = "${aws_network_interface.{{ name }}.security_groups}"
}

output "attachment" {
  value = "${aws_network_interface.{{ name }}.attachment}"
}

output "source_dest_check" {
  value = "${aws_network_interface.{{ name }}.source_dest_check}"
}

output "tags" {
  value = "${aws_network_interface.{{ name }}.tags}"
}
