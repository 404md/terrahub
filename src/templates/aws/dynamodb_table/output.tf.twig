# Define list of variables to be output

output "arn" {
  value = "${aws_dynamodb_table.{{ name }}.arn}"
}

output "id" {
  value = "${aws_dynamodb_table.{{ name }}.id}"
}

output "stream_arn" {
  value = "${aws_dynamodb_table.{{ name }}.stream_arn}"
}

output "stream_label" {
  value = "${aws_dynamodb_table.{{ name }}.stream_label}"
}
