# Define list of variables to be output

output "aws_iam_policy_id" {
  value = "${aws_iam_policy.{{ name }}.id}"
}

output "aws_iam_policy_arn" {
  value = "${aws_iam_policy.{{ name }}.arn}"
}

output "aws_iam_policy_name" {
  value = "${aws_iam_policy.{{ name }}.name}"
}

output "aws_iam_policy_path" {
  value = "${aws_iam_policy.{{ name }}.path}"
}

output "aws_iam_policy_policy" {
  value = "${aws_iam_policy.{{ name }}.policy}"
}
