# Define list of variables to be output

output "id" {
  value = "${aws_lb.{{ name }}.id}"
}

output "arn" {
  value = "${aws_lb.{{ name }}.arn}"
}

output "arn_suffix" {
  value = "${aws_lb.{{ name }}.arn_suffix}"
}

output "dns_name" {
  value = "${aws_lb.{{ name }}.dns_name}"
}

output "canonical_hosted_zone_id" {
  value = "${aws_lb.{{ name }}.canonical_hosted_zone_id}"
}

output "zone_id" {
  value = "${aws_lb.{{ name }}.zone_id}"
}
