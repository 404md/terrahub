# Specify default values for variables defined in variables.tf

############
# provider #
############
account_id = "123456789012"
region     = "us-east-1"

#############
# top level #
#############
lambda_function_name = "{{ name }}"
lambda_iam_role      = "{{ name }}"
lambda_description   = "Managed by TerraHub"
lambda_memory_size   = 128
lambda_runtime       = "nodejs6.10"
lambda_timeout       = 3
lambda_publish       = false
lambda_handler       = "index.handler"
lambda_s3_bucket     = "www.terrahub.io"
lambda_s3_key        = "hello-world"
lambda_reserved_concurrent_executions = 0

##############
# vpc config #
##############
lambda_subnet_ids         = []
lambda_security_group_ids = []

#########################
# environment variables #
#########################
lambda_environment_variables = {}

########
# tags #
########
custom_tags  = {}
default_tags = {
  "Name"        = "{{ name }}"
  "ThubCode"    = "{{ code }}"
  "ThubEnv"     = "default"
  "Description" = "Managed by TerraHub"
}

