resource "aws_api_gateway_stage" "{{ name }}" {
  stage_name            = "${var.api_gateway_stage_name}"
  rest_api_id           = "${var.aws_api_gateway_rest_api_id}"
  deployment_id         = "${var.aws_api_gateway_deployment_id}"
  cache_cluster_enabled = "${var.aws_api_gateway_cache_cluster_enabled}"
  cache_cluster_size    = "${var.aws_api_gateway_cache_cluster_size}"
  client_certificate_id = "${var.aws_api_gateway_client_certificate_id}"
  description           = "${var.aws_api_gateway_description}"
  documentation_version = "${var.aws_api_gateway_documentation_version}"
  variables             = "${var.aws_api_gateway_variables}"
}
