# Specify default values for variables defined in variables.tf

############
# provider #
############
account_id                 = "123456789012"
region                     = "us-east-1"

##########
# egress #
##########
network_acl_egress_protocol   = "tcp"
network_acl_egress_rule_no    = 200
network_acl_egress_action     = "allow"
network_acl_egress_cidr_block = "10.3.0.0/18"
network_acl_egress_from_port  = 443
network_acl_egress_to_port    = 443
network_acl_egress_icmp_type  = 0
network_acl_egress_icmp_code  = 0

###########
# ingress #
###########
network_acl_ingress_protocol   = "tcp"
network_acl_ingress_rule_no    = 100
network_acl_ingress_action     = "allow"
network_acl_ingress_cidr_block = "10.3.0.0/18"
network_acl_ingress_from_port  = 80
network_acl_ingress_to_port    = 80
network_acl_ingress_icmp_type  = 0
network_acl_ingress_icmp_code  = 0

########
# tags #
########
custom_tags  = {}
default_tags = {
  "Name"        = "{{ name }}"
  "Description" = "Managed by TerraHub"
  "ThubCode"    = "{{ code }}"
  "ThubEnv"     = "default"
}

