# Define list of variables to be output

output "id" {
  value = "${aws_load_balancer_listener_policy.{{ name }}.id}"
}

output "load_balancer_name" {
  value = "${aws_load_balancer_listener_policy.{{ name }}.load_balancer_name}"
}

output "load_balancer_port" {
  value = "${aws_load_balancer_listener_policy.{{ name }}.load_balancer_port}"
}
