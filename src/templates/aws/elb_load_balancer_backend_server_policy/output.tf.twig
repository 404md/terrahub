# Define list of variables to be output

output "id" {
  value = "${aws_load_balancer_backend_server_policy.{{ name }}.id}"
}

output "load_balancer_name" {
  value = "${aws_load_balancer_backend_server_policy.{{ name }}.load_balancer_name}"
}

output "instance_port" {
  value = "${aws_load_balancer_backend_server_policy.{{ name }}.instance_port}"
}
