# Define list of variables to be output

output "id" {
  value = "${aws_api_gateway_deployment.{{ name }}.id}"
}

output "stage_name" {
  value = "${aws_api_gateway_deployment.{{ name }}.stage_name}"
}

output "invoke_url" {
  value = "${aws_api_gateway_deployment.{{ name }}.invoke_url}"
}

output "execution_arn" {
  value = "${aws_api_gateway_deployment.{{ name }}.execution_arn}"
}

output "created_date" {
  value = "${aws_api_gateway_deployment.{{ name }}.created_date}"
}
