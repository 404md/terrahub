# Define list of variables to be output

output "api_gateway_key_id" {
  value = "${aws_api_gateway_api_key.{{ name }}.id}"
}

output "api_gateway_key_created_date" {
  value = "${aws_api_gateway_api_key.{{ name }}.created_date}"
}

output "api_gateway_key_last_updated_date" {
  value = "${aws_api_gateway_api_key.{{ name }}.last_updated_date}"
}

output "api_gateway_key_value" {
  value = "${aws_api_gateway_api_key.{{ name }}.value}"
}

output "api_gateway_key_name" {
  value = "${aws_api_gateway_api_key.{{ name }}.name}"
}
