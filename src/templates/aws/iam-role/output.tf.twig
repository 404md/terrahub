# Define list of variables to be output

output "iam_arn" {
  value = "${aws_iam_role.{{ name }}.arn}"
}

output "iam_create_date" {
  value = "${aws_iam_role.{{ name }}.create_date}"
}

output "iam_ir_unique_id" {
  value = "${aws_iam_role.{{ name }}.unique_id}"
}

output "iam_name" {
  value = "${aws_iam_role.{{ name }}.name}"
}

output "iam_description" {
  value = "${aws_iam_role.{{ name }}.description}"
}
