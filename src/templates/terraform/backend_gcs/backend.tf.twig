terraform {
  backend "gcs" {
    region  = "{{ provider_region }}"
    bucket  = "{{ provider_bucket }}"
    prefix  = "{{ provider_prefix }}"
    encryption_key = "{{ provider_encrypt }}"
  }
  required_version = "{{ required_version }}"
  provider = "google.backend"
}

provider "google" {
  alias  = "backend"
  region = "{{ provider_region }}"
}
